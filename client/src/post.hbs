<!doctype html>
<html class="no-js" lang="">
    <head>
        <meta charset="utf-8">
        <meta http-equiv="x-ua-compatible" content="ie=edge">
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <title>{{post.title}} | Sonya Falcon</title>
        <meta name="description" content="{{post.summary}}">
        <meta name="author" content="Sonya Falcon">
		    <meta name="keywords" content="{{post.metaKeywords}}">
        <meta property="og:type" content="article">
        <meta property="og:title" content="{{post.title}}" />
        <meta property="og:url" content="https://sonyafalcon.com/{{post.id}}" />
        <meta property="og:image" content="{{post.titleImg}}" />
        <meta property="og:image:width" content="1111" />
        <meta property="og:image:height" content="736" />
        <meta name="twitter:card" content="summary">
        <meta name="twitter:url" content="https://sonyafalcon.com/{{post.id}}">
        <meta name="twitter:title" content="{{post.title}}">
        <meta name="twitter:description" content="{{post.summary}}">
        <meta name="twitter:image" content="{{post.titleImg}}">
        <meta name="twitter:site" content="@{{twitterHandle}}">
        <link rel="canonical" href="https://sonyafalcon.com/{{post.id}}" />
        <link rel="apple-touch-icon" href="apple-touch-icon.png">
        <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css">
        <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/bulma/0.3.2/css/bulma.min.css">
        <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/social-sharing/1.7.2/css/social-sharing.min.css">
        <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/react-tagsinput@3.18.0/react-tagsinput.css">
        <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/lyef-switch-button@1.3.2/css/main.css">
        <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/react-typist@2.0.4/dist/Typist.css">
        <link href="https://fonts.googleapis.com/css?family=Cutive+Mono|Work+Sans" rel="stylesheet">
        <style>
          .switch-button .label {
            display: inline-block;
          }
        </style>
    </head>
    <body>
      <app data-env="{{env}}" id="root">
        <div style="height: 100%;">
          <section class="section">
            <div class="container" style="width: 75%;">
              <a class="title" href="/" style="margin-top: 15px;">Sonya Falcon</a>
              <div style="overflow: hidden; height: auto; width: 100%; margin-top: 15px;"><img src="{{post.titleImg}}"></div>
              <h1 class="title" style="margin-top: 15px;">{{post.title}}</h1>
              {{#if post.page}}
              {{else}}
                <time datetime="{{post.formattedTimeAttr}}" style="font-weight: bold; margin-top: 15px;">{{post.formattedTime}}</time>
              {{/if}}
              <div class="content" style="margin-top: 15px;">
                {{post.html}}
              </div>
            </div>
          </section>
        </div>
      </app>
      <script src="/bundle-{{timestamp}}.js"></script>


        <!-- Google Analytics: change UA-XXXXX-Y to be your site's ID.
        <script>
            window.ga=function(){ga.q.push(arguments)};ga.q=[];ga.l=+new Date;
            ga('create','UA-XXXXX-Y','auto');ga('send','pageview')
        </script>
        <script src="https://www.google-analytics.com/analytics.js" async defer></script> -->
    </body>
</html>
